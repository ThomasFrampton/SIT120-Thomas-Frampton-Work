Vue.createApp({
  data() {
    return {
      selected: 'A',
      options: [
        { text: 'Option A', value: 'A' },
        { text: 'Option B', value: '2' },
        { text: 'Option C', value: 'iii' }
      ]
    }
  }
}).mount('#v-model-select-dynamic')
